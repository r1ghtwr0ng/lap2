{
    "main_supervisor": {
        "name": "lap2_deamon"
    },
    "task_supervisor": {
        "name": "lap2_superv",
        "max_children": 10
    },
    "udp_server": {
        "name": "udp_server",
        "udp_port": 1447,
        "req_timeout": 50000,
        "max_dgram_handlers": 10,
        "queue_interval": 100,
        "max_queue_size": 1000
    },
    "tcp_server": {
        "name": "tcp_server",
        "tcp_port": 3001,
        "req_timeout": 50000,
        "queue_interval": 100,
        "max_queue_size": 1000
    },
    "router": {
        "name": "router",
        "lap2_addr": "test",
        "clove_cache_size": 1000,
        "clove_cache_ttl": 30000,
        "relay_routes_size": 5000,
        "relay_routes_ttl": 1800000,
        "proxy_policy": true,
        "proxy_limit": 20
    }
}